{
  "name": "produtos-api",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "sequelize db:migrate",
    "start": "node server",
    "dev": "NODE_ENV=development nodemon server --ignore ./server/__tests__",
    "build": "ng build",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test npm-run-all run-test run-after-test --continue-on-error",
    "run-test": "NODE_ENV=test jest --runInBand",
    "run-after-test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --output-path dist --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.1",
    "pg": "^8.3.3",
    "rxjs": "~6.6.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
